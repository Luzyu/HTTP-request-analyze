# Optional
MITMPROXY_CER='~/.mitmproxy/mitmproxy-ca-cert.cer'
MITMPROXY_PEM='~/path/to/mitmproxy-ca-cert.pem'
CHROMEDRIVER_ABS_PATH='/absolute/path/to/chromedriver'